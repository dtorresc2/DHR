                <!--                        android:paddingRight="2dp"-->
                <!--                        android:text="Habilitado"-->
                <!--                        android:textColor="@color/Blanco"-->
                <!--                        android:textSize="14sp" />-->
                <!--                </LinearLayout>-->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp"
                    android:weightSum="3">

                    <TextView
                        android:id="@+id/debePaciente"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:paddingLeft="10dp"
                        android:paddingTop="3dp"
                        android:text="C: 15.25"
                        android:textColor="@color/Blanco"
                        android:textSize="12sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/haberPaciente"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:paddingLeft="10dp"
                        android:paddingTop="3dp"
                        android:text="A: 15.25"
                        android:textColor="@color/Blanco"
                        android:textSize="12sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/saldoPaciente"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:paddingLeft="10dp"
                        android:paddingTop="3dp"
                        android:text="S: 15.25"
                        android:textColor="@color/Blanco"
                        android:textSize="12sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <TextView
                    android:id="@+id/estadoPaciente"
                    android:layout_width="25mm"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:layout_weight="1.4"
                    android:background="@color/VerdeOscuro"
                    android:gravity="center_horizontal|center_vertical"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:paddingBottom="1dp"
                    android:text="Habilitado"
                    android:textColor="@color/Blanco"
                    android:textSize="14sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/accionesPaciente"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.7"
                android:background="@color/FondoSecundario2"
                android:src="@drawable/ic_adelante"
                android:text="Limpiar"
                android:textColor="@color/Blanco"
                tools:ignore="SpeakableTextPresentCheck" />
        </LinearLayout>

        <View
            android:id="@+id/separadorPaciente"
            android:layout_width="wrap_content"
            android:layout_height="4dp"
            android:background="@color/FondoSecundario" />

    </LinearLayout>
</FrameLayout>